                                    <li><a href="rentals#motorcycle">MOTORCYCLES</a></li>
                                    <li><a href="rentals#jeep">JEEPS</a></li>
                                    <li><a href="rentals#transportation">TRANSPORTATION</a></li>
                                    <li><a href="rentals#bckcntrygear">BACKCOUNTRY GEAR</a></li>
                                    <li><a href="rentals#camera">VIDEO CAMERAS</a></li>
                                </ul>
                            </li>
                            <!-- Delivery -->
                            <li>
                                <a href="delivery">
                                    WHERE WE DELIVER
                                </a>
                            </li>
                            <!-- MEDIA -->
                            <li class="dropdown">
                                <a href="media" >
                                    MEDIA
                                </a>
                                <ul class="dropdown-menu">
                                    <li><a href="media#photo">PHOTOGRAPHY</a></li>
                                    <li><a href="media#video">VIDEO</a></li>
                                </ul>
                            </li>
                            <!-- Reservation -->
