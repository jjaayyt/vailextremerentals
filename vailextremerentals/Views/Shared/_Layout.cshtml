                                <a href="reservations">
                                    MAKE A RESERVATION
                                </a>
                            </li>
                            <!-- Contact -->
                            <li>
                                <a href="contact">
                                    CONTACT US
                                </a>
                            </li>
                        </ul>
                    </div><!--/end container-->
                </div><!--/navbar-collapse-->
            </div>
            <!-- End Navbar -->
        </div>
        <!--=== End Header v4 ===-->

        @RenderBody()

        <!--=== Footer Version 1 ===-->
        <div class="footer-v1">
            <div class="footer">
                <div class="container">
                    <div class="row">
             
                        <div class="col-md-4 map-img md-margin-bottom-40">
                            <div class="headline"><h2>Contact</h2></div>
                            <address class="md-margin-bottom-40">
                                VAIL EXTREME RENTALS<br />
                                <i class="fa fa-map-marker"></i>&nbsp;&nbsp;2095 Zermatt Lane Unit A, Vail, Colorado 81657<br />
                                <i class="fa fa-phone"></i>&nbsp;&nbsp;1 (970) 470-2166<br />
                                 <i class="fa fa-envelope"></i>&nbsp;&nbsp;<a href="contact">vailextremerentals@gmail.com</a>
                            </address>
                        </div><!--/col-md-3-->
                       
                        <div class="col-md-4 md-margin-bottom-40">
                            <a href="/"><img style="display: block; margin-left:auto; margin-right:auto" id="logo-footer" class="footer-logo" src="~/images/logo2-default.gif" alt="" height="200"></a>
                        </div>
                        <div class="col-md-4 md-margin-bottom-40">
                            <div class="headline"><h2>Links</h2></div>
                            <ul class="list-unstyled link-list">
