    <link rel="stylesheet" href="~/css/cubeportfolio.min.css">
    <link rel="stylesheet" href="~/css/custom-cubeportfolio.css">
    <link rel="stylesheet" href="~/css/custom-hover-effects.css" />
    <link rel="stylesheet" href="~/css/hover-min.css" />
</head>
<body>
    <div class="wrapper">
        <!--=== Header v4 ===-->
        <div class="header-v4">
